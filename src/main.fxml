<?import javafx.scene.control.Spinner?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="226.0" prefWidth="228.0" spacing="10" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MainController">
    <padding>
        <Insets bottom="10" left="10" right="10" top="10" />
    </padding>
    <Canvas fx:id="canvas" VBox.vgrow="ALWAYS">
    </Canvas>
   <Pane prefHeight="183.0" prefWidth="208.0">
      <children>
          <Button layoutX="168.0" layoutY="72.0" onAction="#handleBtn" prefHeight="40.0" prefWidth="40.0" text="FIRE" textAlignment="CENTER">
              <font>
                  <Font name="System Bold" size="11.0" />
              </font></Button>
          <Label layoutX="21.0" layoutY="11.0" text="Azimut:" />
            <Spinner fx:id="azimuthSp" editable="true" layoutX="39.0" layoutY="28.0" prefHeight="25.0" prefWidth="100.0" />
          <Label layoutX="23.0" layoutY="60.0" text="Elevace:" />
            <Spinner fx:id="elevationSp" editable="true" layoutX="39.0" layoutY="79.0" prefHeight="25.0" prefWidth="100.0" />
          <Label layoutX="21.0" layoutY="110.0" text="Rychlost:" />
            <Spinner fx:id="speedSp" editable="true" layoutX="39.0" layoutY="127.0" prefHeight="25.0" prefWidth="100.0" />
          <Label layoutX="143.0" layoutY="23.0" prefHeight="25.0" prefWidth="5.0" text="°">
              <font>
                <Font size="17.0" />
            </font>
          </Label>
          <Label layoutX="143.0" layoutY="69.0" prefHeight="25.0" prefWidth="5.0" text="°">
             <font>
                <Font size="17.0" />
            </font>
         </Label>
          <Label  layoutX="143.0" layoutY="131.0" text="m/s" />
      </children>
   </Pane>
</VBox>
